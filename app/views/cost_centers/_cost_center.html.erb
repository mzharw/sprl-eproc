<div id="<%= dom_id cost_center %>">
  <p>
    <strong>Cost center:</strong>
    <%= cost_center.cost_center_id %>
  </p>

  <p>
    <strong>Ctrl name:</strong>
    <%= cost_center.ctrl_name %>
  </p>

  <p>
    <strong>Desc:</strong>
    <%= cost_center.desc %>
  </p>

  <p>
    <strong>Position:</strong>
    <%= cost_center.position %>
  </p>

  <p>
    <strong>Payroll:</strong>
    <%= cost_center.payroll_id %>
  </p>

  <p>
    <strong>Valid to:</strong>
    <%= cost_center.valid_to %>
  </p>

  <p>
    <strong>Created by:</strong>
    <%= cost_center.created_by %>
  </p>

  <p>
    <strong>Updated by:</strong>
    <%= cost_center.updated_by %>
  </p>

  <p>
    <strong>Purch group:</strong>
    <%= cost_center.purch_group_id %>
  </p>

</div>
