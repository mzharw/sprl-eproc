<!DOCTYPE html>
<html>
<head>
  <title>SprlEproc</title>
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
  <%= javascript_importmap_tags %>
</head>

<body>
<%= yield %>
<%= turbo_frame_tag 'toasts', class: "toast-container bottom-0 end-0 p-3" do %>
  <%= render partial: 'shared/toast' %>
  <div class="toast-container bottom-0 end-0 p-3" id="toasts">
  </div>
<% end %>
</body>
</html>
