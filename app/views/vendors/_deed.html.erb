<% vendor.each_with_index do |v, i| %>
  <div class="d-flex">
    <div class="bg-primary-subtle align-content-center border-end text-center" style="width: 35px"><%= i + 1 %></div>
    <div class="w-100">
      <table class="table">
        <tr>
          <td style="width: 150px"><strong>Deed Type</strong></td>
          <td><%= get_value(v, 'deed_type', 'name') %></td>
        </tr>
        <tr>
          <td><strong>Deed Number</strong></td>
          <td><%= get_value(v, 'number') %></td>
        </tr>
        <tr>
          <td><strong>Deed Date</strong></td>
          <td><%= format_datetime(get_value(v, 'issued_date'), false) %></td>
        </tr>
        <tr>
          <td><strong>Notary name</strong></td>
          <td><%= get_value(v, 'notary_full_name') %></td>
        </tr>
        <tr>
          <td><strong>Notary name</strong></td>
          <td><%= get_value(v, 'notary_full_name') %></td>
        </tr>
        <tr>
          <td><strong>SK Menkumham No.</strong></td>
          <td><%= get_value(v, 'ack_letter_number') %></td>
        </tr>
        <tr>
          <td><strong>SK Menkumham Date</strong></td>
          <td><%= get_value(v, 'ack_letter_date') %></td>
        </tr>
        <tr>
          <td><strong>Company Status Based on Deed</strong></td>
          <td><%= get_value(v, 'deed_based_company_type', 'name') %></td>
        </tr>
        <tr>
          <td><strong>Attachment</strong></td>
          <td><%= link_to 'View attachment file', get_value(v, 'files', 'ext_url') %></td>
        </tr>
      </table>
    </div>
  </div>
<% end %>