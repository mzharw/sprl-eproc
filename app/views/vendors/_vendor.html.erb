<div class="d-flex gap-2">
  <div style="width: 250px">
    <div class="accordion " id="vendor_links">
      <%= render 'vendor_links' %>
    </div>
  </div>
  <div class="w-100">
    <table class="table">
      <tr>
        <td><strong>Company Type</strong></td>
        <td><%= vendor.vendor_company_type %></td>
      </tr>
      <tr>
        <td><strong>Company name</strong></td>
        <td><%= vendor.vendor_name %></td>
      </tr>
      <tr>
        <td><strong>Registration No</strong></td>
        <td><%= vendor.code %></td>
      </tr>
      <tr>
        <td><strong>CIVD Registration No</strong></td>
        <td><%= vendor.vendor_civd_reg_id %></td>
      </tr>
    </table>
    <div id="<%= dom_id vendor %>" class="w-100">
    </div>
  </div>
</div>
